{
    "version": "0.2",
    "language": "en-US",
    "allowCompoundWords": false,
    "globRoot": ".",
    "ignorePaths": [
      "**/.*.json",
      ".*ignore",
      "composer.lock",
      "assets/vendor/**",
      "misc/jquery.form.js",
      "lib/Drupal/Component/Diff/**",
      "lib/Drupal/Component/Transliteration/data/**",
      "lib/Drupal/Core/File/MimeType/ExtensionMimeTypeGuesser.php",
      "**/LICENSE.txt",
      "../**/LICENSE.txt",
      "node_modules/*",
      "tests/fixtures/files/*",
      "COPYRIGHT.txt",
      "MAINTAINERS.txt",
      "package.json",
      "yarn.lock",
      ".yarnrc.yml",
      ".yarn/*",
      "phpstan*"
    ],
    "ignoreRegExpList": [
      "^msgstr .*",
      "!!binary .*",
      "%[0-9][0-9A-F]",
      "\\Wi18n",
      "\\x{[0-9A-F]{4,5}}"
    ],
    "dictionaries": [
      "dictionary",
      "companies",
      "fonts",
      "html",
      "php",
      "softwareTerms"
    ],
    "dictionaryDefinitions": [
      {
        "name": "dictionary",
        "path": "./dictionary.txt"
      }
     ],
    "flagWords": [
      "e-mail",
      "grey",
      "hte",
      "ist",
      "queuing"
    ]
}
